{
  "expo": {
    "name": "Manufacture App",
    "slug": "app-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#000000",
      "image": "./assets/brand/splash-logo.png",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.manufactureapp.frontend",
      "adaptiveIcon": {
        "backgroundColor": "#1C1C1C"
      },
      "usesCleartextTraffic": true
    },
    "web": {
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "dd80ff5b-6e67-4ea6-acf8-2ae5d3455602"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
