{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "dev",
        "EXPO_PUBLIC_API_URL": "http://3.108.52.140/api"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "dev",
        "EXPO_PUBLIC_API_URL": "http://3.108.52.140/api"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "prod",
        "EXPO_PUBLIC_API_URL": "https://api.manufactureapp.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
